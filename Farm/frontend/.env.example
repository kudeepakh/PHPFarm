# ====================
# Frontend Environment Configuration
# ====================
# Copy this file to .env and update with your settings
# NEVER commit .env files to version control

# ====================
# API Configuration
# ====================
VITE_API_URL=http://localhost:8000
VITE_API_PREFIX=/api
VITE_API_VERSION=v1

# ====================
# Application
# ====================
VITE_APP_NAME=PHPFrarm
VITE_APP_ENV=development
VITE_APP_DEBUG=true

# ====================
# Authentication
# ====================
VITE_AUTH_TOKEN_KEY=phpfrarm_token
VITE_AUTH_REFRESH_TOKEN_KEY=phpfrarm_refresh_token
VITE_AUTH_USER_KEY=phpfrarm_user

# ====================
# Features
# ====================
VITE_ENABLE_REGISTRATION=true
VITE_ENABLE_SOCIAL_LOGIN=false
VITE_ENABLE_PHONE_AUTH=true
VITE_ENABLE_EMAIL_OTP=true

# ====================
# Analytics (Optional)
# ====================
VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_ID=

# ====================
# Sentry (Optional)
# ====================
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development
